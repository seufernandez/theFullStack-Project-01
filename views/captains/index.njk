{% extends "layout.njk" %}


{% block content %}
    <div class="card table-container">
        <table width="100%">
            <thead>
            
                <tr>
                    <th>Captain</th>
                    <th>Ocupation</th>
                    <th>Action</th>
                </tr>
            </thead>

            <tbody>
            {% for captain in captains %}
                <tr>
                    <td>{{captain.name}}</td>
                    <td>{{captain.ocupation}}</td>
                </tr>
            {% endfor %}
            </tbody>



        </table>
    </div>
{% endblock %}